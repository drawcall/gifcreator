{
  "name": "gifcreator",
  "version": "0.1.3",
  "description": "A Node.js GIF creation tool",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint ./lib --ext .js",
    "travis": "npm run lint",
